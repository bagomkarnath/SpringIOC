<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="listObject" class="com.spring.beans.ci.third.CollectionExample">  
	<constructor-arg>
		<list>
			<value>Apple</value>
			<value>Orange</value>
			<value>Dragon Fruit</value>
		</list>
	</constructor-arg>
</bean>  

<bean id="setObject" class="com.spring.beans.ci.third.CollectionExample">  
	<constructor-arg>
		<set>
			<value>Blue</value>
			<value>Red</value>
			<value>Green</value>
		</set>
	</constructor-arg>
</bean>

<bean id="mapObject" class="com.spring.beans.ci.third.CollectionExample">  
	<constructor-arg>
		<map>
			<entry key="name" value="Harish"></entry>
			<entry key="stream" value="Computer Science"></entry>
			<entry key="roll" value="202208010012"></entry>
		</map>
	</constructor-arg>
</bean>
</beans> 